<h2 class="white-text">Welcome admin here is your data</h2>

{{#if data}}
{{#each data}}





<table class="centered highlight white black-text" style="margin-bottom: 5%;">
    <thead>
        <tr>
            <th>Detail Name</th>
            <th>Detail Item</th>
        </tr>
    </thead>
    <tbody>

        <tr>
            <td>Patient Name</td>
            <td>{{patientName}}</td>
        </tr>

        <tr>
            <td>age</td>
            <td>{{age}}</td>
        </tr>
        <tr>
            <td>city</td>
            <td>{{city}}</td>
        </tr>
        <tr>
            <td>state</td>
            <td>{{state}}</td>
        </tr>
        <tr>
            <td>hospital address</td>
            <td>{{hospital}}</td>
        </tr>
        <tr>
            <td>Blood-Group</td>
            <td>{{bloodGroup}}</td>
        </tr>
        <tr>
            <td>number_of_cylinders</td>
            <td>{{number_of_cylinders}}</td>
        </tr>
        <tr>
            <td>phone</td>
            <td>{{phone}}</td>
        </tr>
        <tr>
            <td>Email</td>
            <td>{{email}}</td>
        </tr>
        <tr>
            <td>date</td>
            <td>{{date}}</td>
        </tr>


    </tbody>
</table>

{{/each}}
{{/if}}